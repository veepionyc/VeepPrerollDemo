source 'https://github.com/CocoaPods/Specs.git'
source 'https://github.com/veepionyc/Specs.git'

platform :ios, '9.0'

use_frameworks!

target "VeepPreroll" do
    pod 'GoogleAds-IMA-iOS-SDK', '~> 3.9'
    pod 'VPKit/Core'  , :path => '../VPKit_7'
end

target "VeepPrerollC" do
pod 'GoogleAds-IMA-iOS-SDK', '~> 3.9'
pod 'VPKit/Core'  , :path => '../VPKit_7'
pod 'Fabric'
pod 'Crashlytics'
end

target "VeepPrerollB" do
pod 'GoogleAds-IMA-iOS-SDK', '~> 3.9'
end

target "VeepPrerollBC" do
pod 'Fabric'
pod 'Crashlytics'
pod 'GoogleAds-IMA-iOS-SDK', '~> 3.9'
end
